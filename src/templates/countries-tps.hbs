<div class="country-card">
    {{#each this}}
    <h2 class="country-name">{{this.nativeName}}</h2>
    <p class="country-text">Capital: {{this.capital}}</p>
    <p class="country-text">Population: {{this.population}}</p>
        <ul class="languages-list">
       {{#each this.languages}}
            <li class="language-item">{{this.name}}</li>
       {{/each}}
    </ul>
    <img src={{this.flag}} alt={{this.nativeName}}>
    {{/each}}
</div>